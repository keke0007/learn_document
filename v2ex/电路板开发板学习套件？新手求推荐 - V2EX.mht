@charset "utf-8";

:root { --box-padding: 5px; --component-margin: 5px; }

body { text-size-adjust: none; }

h1 { font-size: 22px; font-weight: 500; line-height: 120%; margin: 5px 0px 10px; padding: 0px; }

#Wrapper .content { min-width: 300px; width: auto; padding: 5px; }

#Bottom .content { min-width: 310px; width: auto; }

.header { padding: 5px; }

.cell { padding: 5px; word-break: break-word; }

.cell_ops { padding: 6px; }

.subtle { padding: 5px; }

.inner { padding: 5px; word-break: break-word; }

.caution { padding: 5px; }

.sl { width: 210px; font-size: 16px; }

.ml { width: 210px; }

.mll { width: 100%; height: initial; padding: 5px; box-sizing: border-box; font-size: 16px; }

.sls { font-size: 16px; }

.mle { width: 100%; padding: 5px; box-sizing: border-box; font-size: 16px; }

.onoffswitch { width: 90px; }

.inline-input-group { justify-content: space-between; }

.tall { height: 220px; }

.imgly { max-width: 280px; }

a.tab:active, a.tab:link, a.tab:visited { display: inline-block; font-size: 13px; line-height: 16px; padding: 5px 6px; margin-right: 5px; border-radius: 3px; color: rgb(85, 85, 85); }

a.tab:hover { background-color: rgb(245, 245, 245); color: rgb(0, 0, 0); text-decoration: none; }

a.tab_current:active, a.tab_current:link, a.tab_current:visited { display: inline-block; font-size: 13px; line-height: 16px; padding: 5px 6px; margin-right: 5px; border-radius: 3px; background-color: rgb(51, 51, 68); color: rgb(255, 255, 255); }

a.tab_current:hover { background-color: rgb(68, 68, 85); color: rgb(255, 255, 255); text-decoration: none; }

.thank_area > a { visibility: visible; }

.imgly { max-width: 100%; }

.markdown_body > p > img { max-width: 100%; }

.editor_syntax_label_left { display: none; }

.editor_syntax_label_right { display: none; }

.select2-search__field { font-size: 16px; }

.status_editor { font-size: 16px; }

.note_editor { max-height: calc(-260px + 100vh); font-size: 16px; }

.settings.privacy .privacy-settings-form > div > .settings-explanation { grid-area: 2 / 1 / 3 / 3; }

.settings.privacy .privacy-settings-form > div > span { justify-self: start; }

.box-transparent { margin-bottom: 5px; }

.member-activity-bar { width: 100%; }

.avatar.mobile { width: 32px; height: 32px; vertical-align: middle; border-radius: 32px; }

.menu-guest.mobile { width: 32px; height: 32px; vertical-align: middle; }

.member-activity-mobile-wrapper { padding: 10px; }

#search-container { flex: 0 0 100px; margin: 0px 10px; padding-left: 26px; }

#search-container.active { flex: 1 0 100px; }

#site-header-logo { margin-right: auto; }

.box-title { margin-bottom: 5px; }

#my-nodes { grid: none / 1fr 1fr; }

.no-fav-nodes { }

.no-fav-nodes > .info { margin: 10px 0px 0px; }

.image-grid { grid: auto-flow 160px / repeat(2, 50%); }

.image-view img { max-width: 270px; }

.image-share li { list-style: none; margin-top: 10px; display: block; }

.image-share li input { display: block; margin-top: 5px; width: calc(100% - 12px); }

.image-edit { flex-flow: column; }

.image-edit .image-edit-info { display: flex; width: 100%; }

.sep { height: var(--component-margin); }

.status { padding: 5px; }

.status_avatar { width: 32px; height: 32px; }

.status_avatar .avatar { width: 32px; height: 32px; }

.status_main { margin: 0px 0px 0px 40px; }
