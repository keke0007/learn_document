.onoffswitch { position: relative; width: 120px; min-width: 90px; max-width: 120px; user-select: none; }

.onoffswitch label { display: block; overflow: hidden; cursor: pointer; border-radius: 20px; }

.onoffswitch label .frame { width: 200%; margin-left: -100%; transition: margin 0.15s ease-in; }

.onoffswitch label .frame::after, .onoffswitch label .frame::before { box-sizing: border-box; float: left; width: 50%; height: 30px; padding: 0px; line-height: 30px; font-size: 14px; color: rgb(255, 255, 255); font-family: Trebuchet, Arial, sans-serif; font-weight: 700; }

.onoffswitch label .frame::before { content: "ON"; padding-left: 16px; background-color: rgb(104, 201, 68); color: rgb(255, 255, 255); }

.onoffswitch label .frame::after { content: "OFF"; padding-right: 16px; background-color: rgb(224, 224, 224); color: rgb(153, 153, 153); text-align: right; }

.onoffswitch label .switch { width: 18px; margin: 6px; background: rgb(255, 255, 255); border-radius: 20px; position: absolute; top: 0px; bottom: 0px; left: 0px; transition: 0.15s ease-in; }

.onoffswitch input[type="checkbox"] { display: none; }

.onoffswitch input[type="checkbox"]:checked + label .frame { margin-left: 0px; }

.onoffswitch input[type="checkbox"]:checked + label .switch { left: calc(100% - 32px); }

