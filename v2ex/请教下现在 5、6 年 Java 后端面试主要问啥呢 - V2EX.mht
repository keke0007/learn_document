@charset "utf-8";

.image-grid { display: grid; grid: auto-flow 160px / repeat(4, 25%); place-items: center; }

.image-grid a.image-grid-item { display: flex; align-items: center; justify-content: space-around; border-radius: var(--box-border-radius); padding: 10px; width: calc(100% - 20px); height: calc(100% - 20px); transition: background-color 0.2s; background-color: rgb(255, 255, 255); }

.image-grid a.image-grid-item:hover { background-color: rgb(240, 240, 240); }

.image-grid a.image-grid-item img { width: 100%; height: 100%; object-fit: scale-down; }

.cell.image-title h2 { margin: 0px; padding: 5px 0px; border: none; font-weight: 400; font-size: 16px; line-height: 130%; word-break: break-word; }

.cell.image-title .pixelated-toggle { min-width: 200px; margin-left: 10px; }

.image-info { background-image: url("/static/img/shadow_light.png"); background-repeat: repeat-x; }

.image-view { display: flex; box-sizing: border-box; align-items: center; justify-content: space-around; min-width: 160px; min-height: 160px; width: fit-content; max-width: 100%; margin: auto; padding: 5px; text-decoration: none; border-radius: var(--box-border-radius); transition: background-color 0.2s; background-color: rgb(255, 255, 255); }

.image-view:hover { background-color: rgb(240, 240, 240); }

.image-view img { display: block; max-width: 100%; image-orientation: from-image; }

.image-admin-info > * { display: block; }

